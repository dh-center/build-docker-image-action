{
  "plugins": [
    "jest",
    "@typescript-eslint"
  ],
  "extends": [
    "plugin:jest/recommended",
    "codex"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "env": {
    "node": true,
    "es6": true,
    "jest/globals": true
  }
}